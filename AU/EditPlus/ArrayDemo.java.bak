class ArrayDemo 
{
	public static void main(String[] args) 
	{
		//creating array
		int arr[]=new int[5];

		//store value in array
		arr[0]=10;
		arr[1]=20;
		arr[2]=30;
		arr[3]=40;
		arr[4]=50;
		
		//store data in var
		int a=10;
		System.out.println(a);

		System.out.println(arr[0]); //accessing 0th index ele
		System.out.println(arr[2]);
		System.out.println(arr[4]);



		System.out.println("Hello World!");
	}
}
